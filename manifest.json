{
   "author": "Tim 'timbru31' Brust",
   "background": {
//      "scripts": [ "js/background.js" ]
   },
    "content_scripts": [{
        "exclude_globs": [],
        "include_globs": ["*"],
        "js": [
            "js/lib/jquery-2.1.0.min.js",
            "js/inPage/save.history.js"

        ],
//        "matches": ["https://jira.*/*"],
        "matches": ["*://*/*"]
//        "run_at": "document_end"
    }],
   "browser_action": {
      "default_icon": {
         "16": "images/icon16.png",
         "20": "images/icon20.png",
         "25": "images/icon25.png",
         "30": "images/icon30.png",
         "32": "images/icon32.png",
         "40": "images/icon40.png",
         "48": "images/icon48.png",
         "64": "images/icon64.png"
      },
      "default_popup": "html/popup.html",
      "default_title": "__MSG_defaultTitle__"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Ctrl+Shift+K",
            "default": "Ctrl+Shift+K",
            "linux": "Ctrl+Shift+K",
            "mac": "Command+Shift+K",
            "windows": "Ctrl+Shift+K"
         }
      },
      "open-ticket-in-current-tab": {
         "description": "__MSG_openInCurrentTab__",
         "suggested_key": {
            "chromeos": "Alt+K",
            "default": "Alt+K",
            "linux": "Alt+K",
            "mac": "Alt+K",
            "windows": "Alt+K"
         }
      },
      "open-ticket-in-new-tab": {
         "description": "__MSG_openInNewTab__",
         "suggested_key": {
            "chromeos": "Alt+Shift+K",
            "default": "Alt+Shift+K",
            "linux": "Alt+Shift+K",
            "mac": "Alt+Shift+K",
            "windows": "Alt+Shift+K"
         }
      }
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "64": "images/icon64.png"
   },
   "manifest_version": 2,
//   "minimum_chrome_version": "49",
   "name": "History links",
   "omnibox": {
      "keyword": "history"
   },
   "options_ui": {
      "chrome_style": true,
      "page": "html/options.html"
   },
   "permissions": [ "activeTab", "contextMenus", "storage" ],
   "short_name": "links",
//   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.0"
}
